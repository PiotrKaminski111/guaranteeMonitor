{% extends 'base.html.twig' %}

{% block body %}
    <div class="{{ wrapperClass }}-wrapper" style="float:left">
        {{ label }}
        {{ form_start(form) }}
            <fieldset class="form-group">
                {{ form_widget(form) }}
            </fieldset>
        {{ form_end(form) }}
    </div>

    {% if not form.vars.valid %}
        <div class="alert alert-error">
            {{ form_errors(form) }}
        </div>
    {% endif %}
   
    <div style="clear: both;">
    <a class="btn btn-primary" href="{{ path('homepage') }}">Back to homepage</a>
{% endblock %}


{% block javascripts %}
    
{% endblock %}